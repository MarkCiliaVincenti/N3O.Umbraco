{
  "$schema": "./umbraco/config/appsettings-schema.json",
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=(local);Database=DemoSite.Website;Integrated Security=sspi;"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            }
          ]
        }
      }
    ]
  },
  "uSync": {
    "Settings": {
      "ImportAtStartup": "None",
      "ExportAtStartup": "All",
      "ExportOnSave": "All",
      "UiEnabledGroups": "All",
      "FailOnMissingParent": false
    }
  },
  "Umbraco": {
    "CMS": {
      "Hosting": {
        "Debug": true
      },
      "ModelsBuilder": {
        "AcceptUnsafeModelsDirectory": true,
        "ModelsNamespace": "DemoSite.Core.Content",
        "ModelsDirectory": "~/../DemoSite.Core/Content/Models/Generated"
      }
    }
  }
}